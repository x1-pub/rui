{
  "name": "@x1.pub/rui",
  "version": "0.0.8",
  "description": "Rui is a TypeScript-based Node.js server-side framework",
  "type": "module",
  "types": "./http/index.d.ts",
  "main": "./http/index.cjs",
  "module": "./http/index.mjs",
  "exports": {
    ".": {
      "types": "./http/index.d.ts",
      "require": "./http/index.cjs",
      "import": "./http/index.mjs"
    },
    "./http": {
      "types": "./http/index.d.ts",
      "require": "./http/index.cjs",
      "import": "./http/index.mjs"
    },
    "./http2": {
      "types": "./http2/index.d.ts",
      "require": "./http2/index.cjs",
      "import": "./http2/index.mjs"
    },
    "./http2s": {
      "types": "./http2s/index.d.ts",
      "require": "./http2s/index.cjs",
      "import": "./http2s/index.mjs"
    },
    "./https": {
      "types": "./https/index.d.ts",
      "require": "./https/index.cjs",
      "import": "./https/index.mjs"
    }
  },
  "scripts": {
    "dev": "nodemon",
    "test": "NODE_OPTIONS=--no-warnings node --loader ts-node/esm --test __test__/*.test.ts",
    "build": "rm -rf dist & rollup -c",
    "lint": "standard --fix src/**/*.ts",
    "prepare": "npm run build"
  },
  "repository": "x1-pub/rui",
  "keywords": [
    "node",
    "http",
    "application",
    "framework",
    "middleware",
    "rui"
  ],
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/formidable": "^3.4.5",
    "@types/node": "^18.19.119",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "nodemon": "^3.1.10",
    "rollup": "^4.45.1",
    "rollup-plugin-dts": "^6.2.1",
    "standard": "^17.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": ">= 20"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ]
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "dependencies": {
    "formidable": "^3.5.4"
  }
}
